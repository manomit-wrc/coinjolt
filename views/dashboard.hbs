<section class="content container-fluid">
    <div class="dashboard-top">
    <div class="row">
        <div class="col-md-6">
            <div class="box box-default">
                <div class="box-header with-border">
                    <h3 class="box-title">Wallets</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table class="table scroll-minus">
                        <thead>
                            <tr>
                                <th>SYMBOL</th>
                                <th>NAME</th>
                                <th>BALANCE</th>
                                <th>USD</th>
                            </tr>
                        </thead>
                    </table>

                    <div class="scroll-dashboard" style="height: 445.15px;">
                        <table class="table">
                            <tr>
                                <td>
                                    <img src="http://dev.coinjolt.com/dashboard/img/usd.png" alt="">
                                </td>
                                <td>DOLLAR (USD)</td>
                                <td>
                                    16,773,490.44
                                </td>
                                <td>
                                    $16,773,490.44
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <img src="http://dev.coinjolt.com/dashboard/img/usd.png" alt="">
                                </td>
                                <td>DOLLAR (USD)</td>
                                <td>
                                    16,773,490.44
                                </td>
                                <td>
                                    $16,773,490.44
                                </td>
                            </tr>
                            {{#each user.currency}}
                            <tr>    
                                <td class="s-s-{{toLowerCase this.display_name}} currency-logo-sprite">
                                    
                                </td>
                                <td>{{this.display_name}} <span style="color:red;">({{toUpperCase this.alt_name}})</span></td>
                                <td class="wallet_crp">
                                        {{checkCurrencies this.alt_name ../user.currencyBalance}}
                                </td>
                                <td class="wallet_dollar_{{this.currency_id}}">
                                    
                                </td>
                            </tr>
                            {{/each}}

                        </table>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <div class="col-md-6 equal-hgt">
            <div class="box equal-box">
                <div style="margin-bottom: 4px;" class="box-header">
                    <h3 class="box-title">Latest News</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body ltnews">
                    <img class="img_' + resp[i].id +'" style="width:100%;" src="dist/img/lading_icon.gif">
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>
    </div>

    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">All Cryptocurrencies</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-striped res-table">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Market Cap</th>
                                <th>Rate</th>
                                <th>Volume (24h)</th>
                                <th>Change (24h)</th>
                            </tr>
                        </thead>
                        <tbody id="currency_table">
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
    </div>
</section>