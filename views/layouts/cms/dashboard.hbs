    <script>
      var maxHeight = 0;
      $('.item.bottom').each(function (i) {
        var height = $('.item.bottom').eq(i).height();
        if (height > maxHeight)
          maxHeight = height;
      });

      $('.item.bottom').height(maxHeight);
    </script>

    <script>
