        <!-- jQuery 3 -->
        <script src="/bower_components/jquery/dist/jquery.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/jquery.validate.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.17.0/additional-methods.js"></script>
        <!-- Bootstrap 3.3.7 -->
        <script src="/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
        <!-- iCheck -->
        <script src="/plugins/iCheck/icheck.min.js"></script>
        <script>
          $(function () {
            $('input').iCheck({
              checkboxClass: 'icheckbox_square-blue',
              radioClass: 'iradio_square-blue',
              increaseArea: '20%' /* optional */
            });
          });
        </script>
        <script>
          $(document).ready(function () {

            $('.login-box-body').show();
            $('.login-box-body').animate({
              'top': '0px'
            }, 500);

            $(".alert").fadeTo(2000, 500).slideUp(500, function () {
              $(".alert").slideUp(500);
            });
            $("#frmSignup").validate({
              rules: {
                email: {
                  required: true
                },
                password: {
                  required: true
                },
                confirm_password: {
                  required: true,
                  equalTo: "#password"
                },
                termcondition: {
                  required: true
                },
                phone: {
                  required: true,
                  number: true
                },
                address: {
                  required: true
                },
                city: {
                  required: true
                },
                state: {
                  required: true
                },
                country: {
                  required: true
                },
                zip: {
                  required: true,
                  number: true
                }
              },
              messages: {
                email: {
                  required: "Please enter email"
                },
                password: {
                  required: "Please enter password"
                },
                confirm_password: {
                  required: "Please enter confirm password",
                  equalTo: "Should be match with password"
                },
                termcondition: {
                  required: "Please check terms and condition box"
                },
                phone: {
                  required: "Please enter phone number",
                  number: "Phone number should be numeric"
                },
                address: {
                  required: "Please enter address"
                },
                city: {
                  required: "Please enter city"
                },
                state: {
                  required: "Please enter state"
                },
                country: {
                  required: "Please enter country"
                },
                zip: {
                  required: "Please enter zip",
                  number: "Zip code should be numeric"
                }
              }
            });

            $('#country').on('change', function () {
              var countryCode = $(this).val();
              if (countryCode === '226') {
                $('#usa_states').css('display', 'block');
                $('#state').css('display', 'none');
              }
              else {
                $('#usa_states').css('display', 'none');
                $('#state').css('display', 'block');
              }
            });

          });
        </script>
        <script>  
          $(document).ready(function() {
            $(".reg-btn").click(function() {
             $(".termcondition-error-wrapper").html('Please check terms and condition box');
           });

            $('#termcondition').on("ifChanged", function(e) {
              if($(this).is(":checked")) {
               $(".termcondition-error-wrapper").hide();
             }
             else {
              $(".termcondition-error-wrapper").show();
            }  
          });


          });
        </script>
        <!-- Starts Scriptd for Responsive Navigation -->
        <script>
          $(document).ready(function() {
            $(".nav-click").click(function() {
              $(".header-top .top-menu ul").slideToggle();
            });
          });
        </script>
        <!-- Ends Scriptd for Responsive Navigation -->

        <script type="text/javascript">
          $(document).ready(function(){
            $.get("http://coincap.io/page/BTC", function(data, status){
              $('#btc').text(data.price_usd.toFixed(2));
            }); 
            $.get("http://coincap.io/page/ETH", function(data, status){

              $('#eth').text(data.price_usd.toFixed(2));
            });
            $.get("http://coincap.io/page/XRP", function(data, status){

              $('#xrp').text(data.price_usd.toFixed(2));
            }); 
            $.get("http://coincap.io/page/LTC", function(data, status){

              $('#ltc').text(data.price_usd.toFixed(2));
            });
            $.get("http://coincap.io/page/BCH", function(data, status){

              $('#bch').text(data.price_usd.toFixed(2));
            });
          });
        </script>

        <!--Starts Login Live Price-->
        <script>
          $(document).ready(function() {
            $(".marketcap-crypto img").click(function() {
              $(".marketcap-crypto ul").slideToggle();
            });
          });
        </script>
        <!--Ends Login Live Price-->

        <script>
          var capitalizeMe = "";
          $(".login-box select#country option").each(function() {

            capitalizeMe = $(this).text();
            $(this).text(capitalizeMe.charAt(0).toUpperCase() + capitalizeMe.substring(1));
          });
        </script>

        <script>
          $(document).ready(function () {

                //  Starts Script  For Capitalize The Anhor Text
                function toTitleCase(str) {
                  return str.replace(/\w\S*/g, function (txt) { return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase(); });
                }
                $("select#country option").each(function () {
                  var capText = $(this).text();
                  $(this).html(toTitleCase(capText));
                });
              });
            </script>

          </body>

