        <script>
          var capitalizeMe = "";
          $(".login-box select#country option").each(function() {

            capitalizeMe = $(this).text();
            $(this).text(capitalizeMe.charAt(0).toUpperCase() + capitalizeMe.substring(1));
          });
        </script>

        <script>
          $(document).ready(function () {

                //  Starts Script  For Capitalize The Anhor Text
                function toTitleCase(str) {
                  return str.replace(/\w\S*/g, function (txt) { return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase(); });
                }
                $("select#country option").each(function () {
                  var capText = $(this).text();
                  $(this).html(toTitleCase(capText));
                });
              });
            </script>

