    <!-- Video-Modal -->
    <div class="modal fade" id="video" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title" id="myModalLabel">Video title</h4>
          </div>
          <div class="modal-body">
            <div>
              <video width="100%" height="100%" poster="dist/frontend_cms/images/poster.jpg" controls>
               <source src="dist/frontend_cms/images/video.mp4" type="video/mp4">
                 <source src="dist/frontend_cms/images/video.ogg" type="video/ogg">
                 </video>
               </div>
               <script src="https://player.vimeo.com/api/player.js"></script>
             </div>
           </div>
         </div>
       </div>
       <!-- Video-Modal -->

       <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
       <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
       <!-- Include all compiled plugins (below), or include individual files as needed -->
       <script src="/dist/frontend_cms/js/bootstrap.min.js"></script>
       <script src="/dist/frontend_cms/js/jquery.touchSwipe.min.js"></script>
       <script src="/dist/frontend_cms/js/bootstrap-touch-slider.js"></script>
       <!-- particles JS -->
       <script src="/dist/frontend_cms/js/particles.js"></script>
       <script src="/dist/frontend_cms/js/app.js"></script>
       <!-- particles JS -->
       <script type="text/javascript">
        $('#bootstrap-touch-slider').bsTouchSlider();
      </script>
      <script src="/dist/frontend_cms/js/jquery.webticker.min.js"></script>
      <!-- Webticker JS -->
      <script>
        $('#webTicker').webTicker();
      </script>
      <!-- OwlCarousel  -->
      <script src="/dist/frontend_cms/js/owl.carousel.js"></script>
      <script>
        $('.owl-carousel').owlCarousel({
          loop:true,
          margin:10,
          nav:true,
          autoplay:true,
          navigation: ["‹","›"],
          responsive:{
            0:{
              items:1
            },
            600:{
              items:3
            },
            1000:{
              items:5
            }
          }
        });
      </script>

      <script type="text/javascript">
        $(document).ready(function(){
          $.get("http://coincap.io/page/BTC", function(data, status){
            $('.btc').text(data.price_usd.toFixed(2));
          }); 
          $.get("http://coincap.io/page/ETH", function(data, status){

            $('.eth').text(data.price_usd.toFixed(2));
          });
          $.get("http://coincap.io/page/XRP", function(data, status){

            $('.xrp').text(data.price_usd.toFixed(2));
          }); 
          $.get("http://coincap.io/page/LTC", function(data, status){

            $('.ltc').text(data.price_usd.toFixed(2));
          });
          $.get("http://coincap.io/page/BCH", function(data, status){

            $('.bch').text(data.price_usd.toFixed(2));
          });
        });
      </script>

      <!-- OwlCarousel  -->

      <!-- STARTS SCROLL TO TOP -->
      <script>
        $(document).ready(function () {

          $(window).scroll(function () {
            if ($(this).scrollTop() > 100) {
              $('.scrollup').fadeIn();
            } else {
              $('.scrollup').fadeOut();
            }
          });

          $('.scrollup').click(function () {
            $("html, body").animate({
              scrollTop: 0
            }, 600);
            return false;
          });

        });
      </script>

