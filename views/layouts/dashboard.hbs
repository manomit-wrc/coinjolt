          var formData = new FormData($('#frmIDChange')[0]);
          $.ajax({
            type: 'POST',
            url: '/update-id-proof',
            data: formData,
            processData: false,
            contentType: false,
            beforeSend:function() {
              $("#loaderimg").show();
            },
            success: function ( response ) {
              $("#loaderimg").hide();
              $(".errorIDProof").show();
              if(response.success === true) {
                $(".errorIDProof").removeClass('error').addClass('success');
                $("#id-proof-view").attr('href', '/id-proof/'+response.file_name);
