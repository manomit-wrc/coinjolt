<section class="content-header">
        <h1>Pending Wire Transfers List</h1>
        <ol class="breadcrumb">
          <li>
            <a href="/dashboard">
              <i class="fa fa-dashboard"></i> Home</a>
          </li>
          <li class="active">Pending Wire Transfers List</li>
        </ol>
</section>

<section class="content container-fluid">
  <!-- Starts Deposit Funds -->
  <div class="deposit-funds">
    <div class="row">
      <div class="col-md-12">
        <div class="box">
          <!-- /.box-header -->
          <div class="box-body">
              	<table id="pending_wire_transfer_table" class="table table-striped res-table">
	                <thead>
	                    <tr>
	                        <th>ID</th>
	                        <th>Name</th>
	                        <th>Amount</th>
	                        <th>Date</th>
	                        <th>Action</th>
	                    </tr>
	                </thead>
	                <tbody>
	                	{{#each all_data}}
	                		<tr>
	                			<td>{{this.id}}</td>
		                        <td>{{this.User.first_name}} {{this.User.last_name}}</td>
		                        <td>{{this.amount_usd}}</td>
		                        <td>{{dateFormat this.createdAt "MM-DD-YY"}}</td>
		                        <td>
		                        	<button value="{{this.id}}" class="btn approve-req">Approve</button>
									<button value="{{this.id}}" class="btn reject-req" id="reject">Reject</button>
								</td>
	                		</tr>
	                	{{/each}}
	                </tbody>
	            </table>
          </div>
  		</div>
                    
      </div>
          <!-- /.box-body -->
    </div>
        <!-- /.box -->
  </div>
  <!-- Ends Deposit Funds -->
</section>