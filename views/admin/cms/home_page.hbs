<section class="content-header">
    <h1>Home Page</h1>
    <ol class="breadcrumb">
        <li><a href="/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
         <li class="active"> CMS- Home page </li>
    </ol>
</section>
<div class="loader"></div>
<section class="content container-fluid">
    <!-- Starts Deposit Funds -->
    <div class="deposit-funds">
      <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title">CMS- Home page details</h3>
                </div>
                <div class="box-body">
                    <form method="post" action="javasript:void(0)" role="form" id="cms-home-page" name="cms-home-page" enctype="multipart/form-data">

                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Subject">Home page banner image upload</label>
                                    <input type="file" class="form-control" name="home_page_banner_image" id="home_page_banner_image" value="">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Subject">How it works image upload</label>
                                    <input class="form-control" type="file" name="how_it_works_image" id="how_it_works_image" value="">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Description">How it works description</label>
                                    <textarea name="how_is_works_description" class="form-control" id="how_is_works_description" placeholder="Enter Description"></textarea>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Subject">Hot wallet image upload</label>
                                    <input class="form-control" type="file" name="hot_wallet_image" id="hot_wallet_image" value="">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Description">Hot wallet description</label>
                                    <textarea name="hot_wallet_desc" class="form-control" id="hot_wallet_desc" placeholder="Enter Description"></textarea>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Subject">cold wallet image upload</label>
                                    <input class="form-control" type="file" name="cold_wallet_image" id="cold_wallet_image" value="">
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Description">cold wallet description</label>
                                    <textarea name="cold_wallet_desc" class="form-control" id="cold_wallet_desc" placeholder="Enter Description"></textarea>
                                </div>
                            </div>

                            <div class="col-md-12">
                                <div class="form-group">
                                    <label for="Description">Upload video</label>
                                    <input type="file" name="video_upload" id="video_upload" class="form-control" value="">
                                </div>
                            </div>

                            <div class="col-md-2 col-md-offset-5">
                                <div class="form-group">
                                    <input class="btn btn-primary" type="button" id="cms_home_page_submit" value="Submit" />
                                </div>
                            </div>
                        </div>

                    </form>
                </div>
            <!-- /.box-body -->
          </div>
    </div>
    <!-- /.box -->
</div>
</div>
</div>
<!-- Ends Deposit Funds -->
</section>

