        <script>
          $(document).ready(function () {

            $('.login-box-body').show();
            $('.login-box-body').animate({
              'top': '0px'
            }, 500);

            //$(".alert").fadeTo(2000, 500).slideUp(500, function () {
              //$(".alert").slideUp(500);
            //});
            $("#frmSignup").validate({
              rules: {
                email: {
                  required: true
                },
                password: {
                  required: true
                },
                confirm_password: {
                  required: true,
                  equalTo: "#password"
                },
                termcondition: {
                  required: true
                },
                phone: {
                  required: true,
                  number: true
                },
                address: {
                  required: true
                },
                city: {
                  required: true
                },
                state: {
                  required: true
                },
                country: {
                  required: true
                },
                zip: {
                  required: true,
                  //number: true
                }
              },
              messages: {
                email: {
                  required: "Enter a valid email email address."
                },
                password: {
                  required: "Enter a password."
                },
                confirm_password: {
                  required: "Confirm password.",
                  equalTo: "Passwords must be the same."
                },
                termcondition: {
                  required: "Check terms and condition box."
                },
                phone: {
                  required: "Enter a phone number.",
                  number: "Phone number a should be numerical values."
                },
                address: {
                  required: "Enter a street address."
                },
                city: {
                  required: "Enter a city."
                },
                state: {
                  required: "Enter a state."
                },
                country: {
                  required: "Enter a country."
                },
                zip: {
                  required: "Enter zip or postal code.",
                  //number: "Zip code should be numeric"
                }
              }
            });

            $('#country').on('change', function () {
              var countryCode = $(this).val();
              if (countryCode === '226') { // usa states
                $('#usa_states').css('display', 'block');
                $('#canada_states').css('display', 'none');
                $('#state').css('display', 'none');
              } else if (countryCode === '240'){ // canada states
                $('#canada_states').css('display', 'block');
                $('#usa_states').css('display', 'none');
                $('#state').css('display', 'none');
              } else {
                $('#usa_states').css('display', 'none');
                $('#canada_states').css('display', 'none');
                $('#state').css('display', 'block');
              }
            });

          });
        </script>
        <script>  
          $(document).ready(function() {

            $(".reg-btn").click(function() {
              $(".termcondition-error-wrapper").html('Check terms and condition box.');
            });

            $('#termcondition').on("ifChanged", function(e) {
              if($(this).is(":checked")) {
                $(".termcondition-error-wrapper").hide();
                $('#terms-modal').modal(); // display term-service modal on clicking term-service checkbox
              }
              else {
               $(".termcondition-error-wrapper").show();
              }
            });

          });
        </script>
